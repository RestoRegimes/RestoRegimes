{
    "name": "lfbac/symfony",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "jlbs/admin-bundle": "dev-master",
        "jlbs/admin-fosuser-bundle": "dev-master",
        "padam87/address-bundle": "~1.0",
        "willdurand/geocoder-bundle": "~4.0.0",
        "egeloen/google-map-bundle": "~2.1",
        "widop/http-adapter-bundle":"~1.0",
        "symfony/framework-bundle": "~2.1",
        "willdurand/geocoder": "~3.1",
        "symfony/console": "~2.1",
        "egeloen/http-adapter": "~0.8",
        "igorw/get-in": "~1.0",
        "symfony/intl": "~2.3",
        "symfony/form": "~2.1",
        "willdurand/faker-bundle": "^1.2",
        "vich/uploader-bundle": "^0.14.0",
        "beberlei/DoctrineExtensions": "^1.0",
        "google/apiclient": "^1.1"

    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "symfony/yaml": "~2.1",
        "maxromanovsky/php-maxmind-geoip": "~1.12",
        "doctrine/cache": "~1.3",
        "doctrine/orm": "~2.3",
        "geoip2/geoip2": "~2.0",
        "symfony/stopwatch": "~2.5",
        "symfony/finder": "~2.1",
        "symfony/browser-kit": "~2.1",
        "satooshi/php-coveralls": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
